-- Slow Motion Border Flow Animation Function
local function StartSlowMotionBorderFlow()
local SIDE_TIME = 1.2 -- SLOW MOTION: Increased from 0.3 to 1.2 seconds per side
local PAUSE_BETWEEN_CYCLES = 2.0 -- SLOW MOTION: Increased pause between cycles

local function resetLines()  
    LineTop.Size = UDim2.new(0, 0, 0, 2)  
    LineRight.Size = UDim2.new(0, 2, 0, 0)  
    LineBottom.Size = UDim2.new(0, 0, 0, 2)  
    LineLeft.Size = UDim2.new(0, 2, 0, 0)  
end  
  
resetLines()  
  
-- Use smoother easing for slow motion  
local tweenInfo = TweenInfo.new(  
    SIDE_TIME,  
    Enum.EasingStyle.Quad, -- Smoother easing for slow motion  
    Enum.EasingDirection.Out,  
    0, -- No repeat  
    false, -- Don't reverse  
    0 -- No delay  
)  
  
while IceButton and IceButton.Parent do  
    -- TOP: dari kiri ke kanan (slow motion)  
    local topTween = TweenService:Create(  
        LineTop,  
        tweenInfo,  
        { Size = UDim2.new(1, 0, 0, 2) }  
    )  
    topTween:Play()  
    topTween.Completed:Wait()  

    -- RIGHT: dari atas ke bawah (slow motion)  
    local rightTween = TweenService:Create(  
        LineRight,  
        tweenInfo,  
        { Size = UDim2.new(0, 2, 1, 0) }  
    )  
    rightTween:Play()  
    rightTween.Completed:Wait()  

    -- BOTTOM: dari kanan ke kiri (slow motion)  
    local bottomTween = TweenService:Create(  
        LineBottom,  
        tweenInfo,  
        { Size = UDim2.new(1, 0, 0, 2) }  
    )  
    bottomTween:Play()  
    bottomTween.Completed:Wait()  

    -- LEFT: dari bawah ke atas (slow motion)  
    local leftTween = TweenService:Create(  
        LineLeft,  
        tweenInfo,  
        { Size = UDim2.new(0, 2, 1, 0) }  
    )  
    leftTween:Play()  
    leftTween.Completed:Wait()  

    resetLines()  
      
    -- Longer pause before next cycle for slow motion effect  
    wait(PAUSE_BETWEEN_CYCLES)  
end

end

